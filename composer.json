{
    "name": "mitratek/livewire-select",
    "description": "Livewire component for searchable select inputs",
    "keywords": ["laravel", "tailwind", "livewire", "select input", "mitratek"],
    "homepage": "https://github.com/mitratek/livewire-select",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ardian Riftha Dhuha",
            "email": "ardian@mitratek.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "livewire/livewire": "^2.0",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Mitratek\\LivewireSelect\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mitratek\\LivewireSelect\\LivewireSelectServiceProvider"
            ],
            "aliases": {
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
